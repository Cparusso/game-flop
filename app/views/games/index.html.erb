<h3>Games:</h3>

<%= form_tag games_path, method: "get" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search Title", title: nil%>
  </p>
<% end %>

<ul>
<% @games.each do |game| %>
  <li><%= link_to game.title, game_path(game.id) %></li><br>
<% end %>
</ul>

<%= button_to "Create Game", new_game_path, method: "get" %>
